{
  "name": "typescript-rest-api-proxy",
  "version": "1.0.0",
  "description": "TypeScript REST API PROXY",
  "main": "dist/server.js",
  "scripts": {
    "test": "jest",
    "watch-ts": "npx tsx watch ./lib/server.ts",
    "start-ts": "node ./dist/server.js"
  },
  "keywords": [
    "proxy",
    "rest",
    "express"
  ],
  "author": "Murat Demirci",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "express": "^4.18.2",
    "jest": "^29.4.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "undici": "^5.19.1",
    "winston": "^3.8.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ]
  }
}
